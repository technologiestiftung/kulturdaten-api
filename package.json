{
	"name": "kulturdaten-api",
	"version": "1.0.0",
	"description": "Wir machen die kulturelle Vielfalt Berlins sichtbar. Dabei hilft ein zentrales Repository f√ºr alle Kulturdaten Berlins.",
	"main": "src/app.js",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint .",
		"lint-openapi": "redocly lint ./src/schemas/kulturdaten.berlin.openapi.generated.yml",
		"typecheck": "tsc --noEmit",
		"dev": "nodemon --inspect",
		"start": "ts-node ./src/app.ts",
		"debug": "export DEBUG=* && npm run dev",
		"test": "vitest test",
		"unit-test": "vitest unit.test",
		"integration-test": "vitest integration.test",
		"schema-to-interface": "rimraf src/generated/* && ts-node ./src/scripts/schema-to-interface.ts",
		"merge-openapi": "swagger-merger -i ./src/schemas/kulturdaten.berlin.openapi.base.yml -o ./src/schemas/kulturdaten.berlin.openapi.generated.yml",
		"generate": "npm run merge-openapi && npm run schema-to-interface && prettier --write **/*.generated.*",
		"seed": "ts-node ./src/scripts/seeder.ts"
	},
	"keywords": [
		"kdb",
		"tsb"
	],
	"author": "Technologiestiftung Berlin",
	"license": "MIT",
	"dependencies": {
		"@types/ip": "^1.1.0",
		"@types/passport": "^1.0.12",
		"@types/passport-http-bearer": "^1.0.37",
		"@types/passport-local": "^1.0.35",
		"@types/swagger-ui-express": "^4.1.3",
		"@types/yamljs": "^0.2.31",
		"ajv": "^8.12.0",
		"ajv-formats": "^2.1.1",
		"argon2": "^0.31.0",
		"axios": "^1.4.0",
		"commander": "^11.0.0",
		"cors": "^2.8.5",
		"debug": "^4.3.4",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-openapi-validator": "^5.0.2",
		"express-validator": "^6.15.0",
		"express-winston": "^4.2.0",
		"ip": "^2.0.0",
		"js-yaml": "^4.1.0",
		"jsonwebtoken": "^9.0.0",
		"mongodb": "^6.0.0",
		"nanoid": "^3.3.6",
		"passport": "^0.6.0",
		"passport-http-bearer": "^1.0.1",
		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.1.13",
		"slug": "^8.2.2",
		"swagger-merger": "^1.5.4",
		"swagger-ui-express": "^4.6.0",
		"ts-node": "^10.9.1",
		"typedi": "^0.10.0",
		"validator": "^13.11.0",
		"winston": "^3.8.2",
		"yamljs": "^0.3.0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "^7.21.0",
		"@babel/plugin-syntax-decorators": "^7.21.0",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.21.0",
		"@faker-js/faker": "^8.0.0",
		"@redocly/cli": "^1.2.0",
		"@types/commander": "^2.12.2",
		"@types/cors": "^2.8.13",
		"@types/debug": "^4.1.7",
		"@types/express": "^4.17.17",
		"@types/js-yaml": "^4.0.5",
		"@types/json-schema-faker": "^0.5.1",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/slug": "^5.0.3",
		"@types/supertest": "^2.0.12",
		"@types/validator": "^13.11.1",
		"@typescript-eslint/eslint-plugin": "6.12.0",
		"eslint": "8.54.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-import": "2.29.0",
		"eslint-plugin-prettier": "5.0.1",
		"json-schema-faker": "^0.5.0-rcv.46",
		"json-schema-to-typescript": "^13.0.0",
		"mongodb-memory-server": "^9.0.0",
		"nodemon": "^3.0.0",
		"rimraf": "^5.0.1",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-mockito": "^2.6.1",
		"tslint": "^6.1.3",
		"typescript": "5.3.2",
		"vitest": "0.34.6"
	}
}
