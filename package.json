{
	"name": "kdb-api",
	"version": "1.0.0",
	"description": "Wir machen die kulturelle Vielfalt Berlins sichtbar. Dabei hilft ein zentrales Repository f√ºr alle Kulturdaten Berlins.",
	"main": "src/app.js",
	"scripts": {
		"dev": "./node_modules/nodemon/bin/nodemon.js --inspect",
		"start": "tsc && node --unhandled-rejections=strict ./dist/app.js",
		"debug": "export DEBUG=* && npm run dev",
		"test": "jest --testPathPattern='test' --detectOpenHandles",
		"unit-test": "jest --testPathPattern='unit.test' --detectOpenHandles",
		"integration-test": "jest --testPathPattern='integration.test' --detectOpenHandles",
		"schema-to-interface": "node ./dist/generatedModels/generator/generator.schema.to.interface.js",
		"merge-openapi": "swagger-merger -i ./src/schemas/kulturdaten.berlin.openapi.base.yml -o ./src/schemas/kulturdaten.berlin.openapi.generated.yml",
		"generate": "tsc && npm run merge-openapi && npm run schema-to-interface",
		"prepare-build": "tsc && npm run schema-to-interface && npm run merge-openapi && npm run test"
	},
	"keywords": [
		"kdb",
		"tsb"
	],
	"author": "Thorsten Diekhof",
	"license": "MIT",
	"dependencies": {
		"@types/ip": "^1.1.0",
		"@types/passport": "^1.0.12",
		"@types/passport-http-bearer": "^1.0.37",
		"@types/passport-local": "^1.0.35",
		"@types/swagger-ui-express": "^4.1.3",
		"@types/yamljs": "^0.2.31",
		"argon2": "^0.30.3",
		"cors": "^2.8.5",
		"debug": "^4.3.4",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-openapi-validator": "^5.0.2",
		"express-validator": "^6.15.0",
		"express-winston": "^4.2.0",
		"ip": "^2.0.0",
		"js-yaml": "^4.1.0",
		"json-schema-to-typescript": "^12.0.0",
		"jsonwebtoken": "^9.0.0",
		"mongodb": "^5.1.0",
		"nanoid": "^4.0.0",
		"passport": "^0.6.0",
		"passport-http-bearer": "^1.0.1",
		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.1.13",
		"slug": "^8.2.2",
		"swagger-merger": "^1.5.4",
		"swagger-ui-express": "^4.6.0",
		"ts-node": "^10.9.1",
		"typedi": "^0.10.0",
		"winston": "^3.8.2",
		"yamljs": "^0.3.0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "^7.21.0",
		"@babel/plugin-syntax-decorators": "^7.21.0",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.21.0",
		"@faker-js/faker": "^7.6.0",
		"@types/cors": "^2.8.13",
		"@types/debug": "^4.1.7",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.4.0",
		"@types/js-yaml": "^4.0.5",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/slug": "^5.0.3",
		"@types/supertest": "^2.0.12",
		"jest": "^29.4.3",
		"nodemon": "^2.0.21",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-jest": "^29.0.5",
		"ts-mockito": "^2.6.1",
		"tslint": "^6.1.3",
		"typescript": "^5.0.0"
	}
}
